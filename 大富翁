#include<stdio.h>
#define w 0.9
#define u 0.1
#define x -51
#define y -0.98
#define z 153

int main () {
    float a,j,k;
    scanf("%f",&a);
    j = w * a  + x;
    k = y * j  + z;
    printf("%.1f ",k);
    for(int i=0;i<4;i++){
         j = w * k + u*j+ x;
         k = y * j  + z;
         printf("%.1f ",k);
    }
    return 0;
}
